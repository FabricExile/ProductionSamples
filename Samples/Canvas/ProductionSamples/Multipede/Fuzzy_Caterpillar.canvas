                      },
                    {
                      "objectType" : "ExecPort",
                      "name" : "in_normal",
                      "nodePortType" : "Out",
                      "execPortType" : "In",
                      "typeSpec" : "Vec3"
                      },
                    {
                      "objectType" : "ExecPort",
                      "name" : "in_gravity",
                      "nodePortType" : "Out",
                      "execPortType" : "In",
                      "typeSpec" : "Vec3"
                      },
                    {
                      "objectType" : "ExecPort",
                      "metadata" : {
                        "uiRange" : "(0, 25)"
                        },
                      "name" : "hairLength",
                      "nodePortType" : "Out",
                      "execPortType" : "In",
                      "typeSpec" : "Scalar"
                      },
                    {
                      "objectType" : "ExecPort",
                      "metadata" : {
                        "uiRange" : "(0, 3)"
                        },
                      "name" : "hairFloppiness",
                      "nodePortType" : "Out",
                      "execPortType" : "In",
                      "typeSpec" : "Scalar"
                      },
                    {
                      "objectType" : "ExecPort",
                      "metadata" : {
                        "uiRange" : "(0, 32)"
                        },
                      "name" : "numSegments",
                      "nodePortType" : "Out",
                      "execPortType" : "In",
                      "typeSpec" : "UInt32"
                      },
                    {
                      "objectType" : "ExecPort",
                      "name" : "result",
