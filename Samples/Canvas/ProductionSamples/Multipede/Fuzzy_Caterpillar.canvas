                }
              ],
            "extDeps" : {
              "Geometry" : "*"
              },
            "code" : "dfgEntry
{  
  // nothing to do?
  if (   numPositions <= 0
      || mesh == null
      || mesh.polygonCount() <= 0
      || mesh.triangleCount() <= 0)
  {
    xfo.resize(0);
    return;
  }
    
  // build array of polygon indices that will be used for scattering.
  UInt32 polyIndices[];
  if (mode == 0)  // use all polygons.
  {
    // set polyIndices[] = all polygon indices.
    polyIndices.resize(mesh.polygonCount());
    for (UInt32 i=0;i<polyIndices.size();i++)
      polyIndices[i] = i;
  }
  else if (mode == 1)  // use all legal polygons in polygonIndices[].
  {
    // set polyIndices[] = all legal polygon indices in polygonIndices[].
    polyIndices.resize(polygonIndices.size());
    UInt32 idx = 0;
    for (UInt32 i=0;i<polygonIndices.size();i++)
      if (polygonIndices[i] < mesh.polygonCount())
        polyIndices[idx++] = polygonIndices[i];
    polyIndices.resize(idx);
  }
  else
  {
    report(\"WARNING (from Scatter): this mode is not implemented\");
  }
  
  // no polygons to scatter from?
  if (polyIndices.size() <= 0)
  {
    xfo.resize(0);
    return;
  }
  
  //
  UInt32 rndPosIdx = 0;
  UInt32 rndOriIdx = 0;
  UInt32 rndSclIdx = 0;
  xfo.resize(numPositions);
  for (UInt32 i=0;i<numPositions;i++)
  {
    // pick a random polygon index in polyIndices[].
    UInt32 polyIdx = polyIndices[floor(polyIndices.size() * mathRandomFloat32(seedPosition, rndPosIdx++))];

    // pick a random sub triangle.
    UInt32 subTriangleIdx = 0;
    if (mesh.getPolygonSubTriangleCount(polyIdx) > 1)
      subTriangleIdx = floor(mesh.getPolygonSubTriangleCount(polyIdx) * mathRandomFloat32(seedPosition, rndPosIdx++));
  
    // get the triangle's vertex indices.
    UInt32 subTriangleVertIdx[3] = mesh.getPolygonSubTrianglePolygonPoints(polyIdx, subTriangleIdx);

    // calculate weights.
    Float32 w[3];
    for (UInt32 k=0;k<3;k++)
    w[k] = 0.001 + mathRandomFloat32(seedPosition, rndPosIdx++);
    
    // normalize weights.
    Float32 wSumInv = 1.0 / (w[0] + w[1] + w[2]);
    for (UInt32 k=0;k<3;k++)
    w[k] *= wSumInv;
    
    // get triangle positions.
    Vec3 trianglePositions[3];
    for (UInt32 k=0;k<3;k++)
      trianglePositions[k] = mesh.getPointPosition(mesh.getPolygonPoint(polyIdx, subTriangleVertIdx[k]));

    // set xfo's position.
    xfo[i].tr.set(0.0);
    for (UInt32 k=0;k<3;k++)
      xfo[i].tr += w[k] * trianglePositions[k];
    
    // set xfo's orientation.
    if (orientation == 1)
    {
      Vec3 direction(0);
      for (UInt32 k=0;k<3;k++)  direction += w[k] * mesh.getPolygonPointNormal(polyIdx, subTriangleVertIdx[k]);
      if (Math_badDivisor(direction.length()))  direction.set(0.0, 0.0, 1.0);
      Vec3 upvector = trianglePositions[1] - trianglePositions[0];
      if (orientationRandomness)
      {
        Quat q;
        q.setFromAxisAndAngle(direction, orientationRandomness * 4.0 * 3.14159 * (-0.5 + mathRandomFloat32(seedOrientation, rndOriIdx++)));
        upvector = q.rotateVector(upvector);
      }
      if (Math_badDivisor(upvector.length()))  upvector.set(0.0, 1.0, 0.0);
      xfo[i].ori.setFromDirectionAndUpvector(direction, upvector);
    }

    // set xfo's scale.
    if (scaleRandomness)  xfo[i].sc.set(scale * (1.0 + scaleRandomness * (-1.0 + 2.0 * mathRandomFloat32(seedScale, rndSclIdx++))));
    else                  xfo[i].sc.set(scale);
  }
}
"
