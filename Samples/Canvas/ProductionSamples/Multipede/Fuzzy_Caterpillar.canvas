                  "extDeps" : {},
                  "code" : "dfgEntry
{
  // constants.
  const Float32 SMALL = 0.000001;

  // init output.
  result.resize(0);

  // check inputs.
  Float32 normalLength = in_normal .length();
  if (    normalLength < SMALL
       || hairLength   < SMALL
       || numSegments  < 4)
    return;
      
  // create positions.
  {
    // resize output.
    result.resize(numSegments);
   
    // normalize gravity and normal vectors.
    Float32 gravityLength = in_gravity.length();
    Vec3    gravity  = in_gravity / (gravityLength < SMALL ? 1.0 : gravityLength);
    Vec3    normal   = in_normal / normalLength;

    // pre-calculate a few things.
    Float32 growStep     = hairLength / (numSegments - 1);
    Float32 growStepInv  = 1.0 / growStep;
    Float32 angleStep    = 0.01 * hairFloppiness * PI * growStep * gravityLength;
    Float32 angleStepCos = cos(angleStep);
    
    // init the first two positions.
    result[0] = position;
    result[1] = position + growStep * normal;
    
    // set the other positions.
    if (gravityLength < SMALL)
    {
      for (UInt32 i=2;i<numSegments;i++)
        result[i] = result[i - 1] + growStep * normal;
    }
    else
    {
      for (UInt32 i=2;i<numSegments;i++)
      {
        Vec3 v = result[i - 1] - result[i - 2];
        Float32 angle = gravity.dot(v * growStepInv);
        if (angle > angleStepCos)
        {
          v = growStep * gravity;
        }
        else
        {
          Quat q;
          if (angle > -1 + SMALL)  q.setFromAxisAndAngle(v.cross(gravity), angleStep);
          else                     q.setFromAxisAndAngle(Vec3(1.0, 0.0, 0.0), angleStep);
          v = q.rotateVector(v);
        }
        result[i] = result[i - 1] + v;
      }
    }
  }
}


