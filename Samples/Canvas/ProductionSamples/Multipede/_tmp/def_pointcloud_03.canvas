  attrName = \"pointSizes\";         Ref<ScalarAttribute> attrPointSizes        = geometryAttributes.getAttribute(attrName, ScalarAttribute);
  if (attrPositions         == null)  {  report(\"warning: pointcloud has no attribute 'positions'\");          return;  }
  if (attrPointOrientations == null)  {  report(\"warning: pointcloud has no attribute 'pointOrientations'\");  return;  }
  if (attrPointVelocities   == null)  {  report(\"warning: pointcloud has no attribute 'pointVelocities'\");    return;  }
  if (attrPointForces       == null)  {  report(\"warning: pointcloud has no attribute 'pointForces'\");        return;  }
  if (attrVertexColors      == null)  {  report(\"warning: pointcloud has no attribute 'vertexColors'\");       return;  }
  if (attrPointSizes        == null)  {  report(\"warning: pointcloud has no attribute 'pointSizes'\");         return;  }

  // set outputs.
  count = geometryAttributes.size();
  positions    = attrPositions;
  orientations = attrPointOrientations;
  velocities   = attrPointVelocities;
  forces       = attrPointForces;
  colors       = attrVertexColors;
  sizes        = attrPointSizes;
}
